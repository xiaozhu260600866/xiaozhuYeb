<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xxxx.mapper.PluginMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xxxx.pojo.Plugin">
        <result column="code" property="code" />
        <result column="name" property="name" />
        <result column="version" property="version" />
        <result column="author" property="author" />
        <result column="config" property="config" />
        <result column="config_value" property="configValue" />
        <result column="desc" property="desc" />
        <result column="status" property="status" />
        <result column="type" property="type" />
        <result column="icon" property="icon" />
        <result column="bank_code" property="bankCode" />
        <result column="scene" property="scene" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        code, name, version, author, config, config_value, desc, status, type, icon, bank_code, scene
    </sql>

</mapper>
